{
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production concurrently 'npm:build:*'",
    "build:page": "eleventy",
    "build:css": "postcss src/css/style.css --output _site/style.css",
    "dev": "NODE_ENV=development concurrently 'npm:dev:*'",
    "dev:page": "eleventy --serve",
    "dev:css": "postcss --watch src/css/style.css --output _site/style.css"
  },
  "dependencies": {
    "@11ty/eleventy": "0.11.1",
    "alpinejs": "2.7.3",
    "autoprefixer": "10.0.2",
    "concurrently": "5.3.0",
    "cssnano": "4.1.10",
    "date-fns": "2.16.1",
    "eleventy-plugin-tailwindcss": "0.2.6",
    "parse5": "6.0.1",
    "parse5-html-rewriting-stream": "6.0.1",
    "postcss": "8.1.8",
    "postcss-cli": "8.3.0",
    "prettier": "2.1.2",
    "tailwindcss": "2.0.1"
  }
}
